#!/bin/bash

# Extract image
#docker load -i ./comp3200image.tar

# Run container
# TODO: add this  -u $(id -u):$(id -g)
# --privileged is needed so we can edit the read only bit of the file system to disable tensorflow crying about numa nodes
docker run --privileged --gpus all -e PASS="not-used" -p 8888:8888 -it --rm -v /data:/tf/comp3200/data --name comp3200image comp3200image
